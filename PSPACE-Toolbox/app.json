{
  "expo": {
    "name": "PSPACE-Toolbox",
    "slug": "pspace-toolbox",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#001B3A"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pspace.toolbox"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#001B3A"
      },
      "package": "com.pspace.toolbox",
      "versionCode": 1
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "a0a71fa3-0fe8-4fdc-b7bf-059f19fd5659"
      }
    }
  }
}